{
  "name": "fdnix",
  "version": "0.1.0",
  "description": "A hybrid search engine for nixpkgs combining semantic vector search with traditional keyword matching",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
    "cdk": "cd packages/cdk && npm install && npx cdk",
    "deploy": "npm run cdk deploy --all",
    "destroy": "npm run cdk destroy --all",
    "diff": "npm run cdk diff",
    "synth": "npm run cdk synth",
    "bootstrap": "npm run cdk bootstrap"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "aws-cdk": "^2.100.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/fdnix.git"
  },
  "license": "MIT",
  "author": "Your Name"
}